@model TranDieuSuong_5101_C1_W2021.Models.Teacher
@{
  ViewBag.Title = "Show";
}

<a class="navlink" href="/Teacher/List">Back To Teachers</a>
<div id="containershow">
  <h2>@Model.TeacherFname @Model.TeacherLname</h2>
  <p>Employee Number: @Model.EmployeeNumber</p>
  <p>Hire date: @Model.HireDate</p>
  <p>Salary: $@Model.Salary</p>
  <p>Class Code: <span class="upperCase">@Model.ClassCode</span></p>
  <p>Class Name: @Model.ClassName</p>
</div>

